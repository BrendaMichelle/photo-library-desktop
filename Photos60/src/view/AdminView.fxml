<AnchorPane prefHeight="550.0" prefWidth="400.0" style="-fx-background-color: #558749 #558749;" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminController">
   <children>
      <ListView fx:id="listOfUsers" layoutX="54.0" layoutY="103.0" prefHeight="324.0" prefWidth="292.0" />
      <TextField fx:id="userNameField" layoutX="108.0" layoutY="437.0" onKeyPressed="#keyPressed" promptText="Username:" />
      <Button fx:id="addUserButton" layoutX="98.0" layoutY="470.0" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="27.0" prefWidth="95.0" style="-fx-background-color: gray;" text="Add User" />
      <Button fx:id="deleteUserButton" layoutX="200.0" layoutY="470.0" mnemonicParsing="false" onAction="#buttonPressed" prefWidth="95.0" style="-fx-background-color: gray;" text="Delete User" />
      <Button fx:id="logoutButton" layoutX="14.0" layoutY="511.0" mnemonicParsing="false" onAction="#buttonPressed" style="-fx-background-color: azure;" text="Logout" />
      <Text layoutX="80.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, Admin!">
         <font>
            <Font size="29.0" />
         </font>
      </Text>
      <Text layoutX="105.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Users on this App:">
